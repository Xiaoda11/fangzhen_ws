<robot name ="my_car">
    <!-- 添加base_footprint -->
    <link name="base_footprint">
        <visual >
            <geometry>
                <sphere radius="0.001"/> 
            </geometry>
            <material name="color">
                <color rgba="0 0 0 1.0"/>
            </material>
        </visual>    
    </link>
    <!-- base_link  半径0.3m 高度0.07 离地0.06-->
    <link name="base_link">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> 
            <geometry>
                <cylinder radius="0.15" length="0.07"/> 
            </geometry>
            <material name="base_link_color">
                <color rgba="0.0 0.0 0.0 1.0"/>               
            </material>
        </visual>       
    </link> 
    <!-- base_link2base_footprint  -->
    <joint name="base_link2base_footprint" type="fixed">
        <origin xyz="0.0 0.0 0.095" rpy="0.0 0.0 0.0"/>
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <axis xyz="0.0 0.0 0.0"/>
    </joint>
    <!-- Laser Radar link -->
     <link name="laser_radar">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> 
            <geometry>
                <cylinder radius="0.025" length="0.04"/> 
            </geometry>
            <material name="laser_radar_color">
                <color rgba="0.7 0.7 0.7 1.0"/>               
            </material>
        </visual>       
    </link>
    <!-- laser_radar2base_link  -->
    <joint name="laser_radar2base_link" type="fixed">
        <origin xyz="0.0 0.0 0.055" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="laser_radar"/>
        <axis xyz="0.0 0.0 0.0"/>
    </joint>

    <!--wheel link -->
     <link name="wheel_1">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> 
            <geometry>
                <sphere radius="0.04"/>
            </geometry>
            <material name="wheel_color">
                <color rgba="0.7 0.7 0.7 1.0"/>               
            </material>
        </visual>       
    </link>
   <link name="wheel_2">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> 
            <geometry>
                <sphere radius="0.04"/>
            </geometry>
            <material name="wheel_color">
                <color rgba="0.7 0.7 0.7 1.0"/>               
            </material>
        </visual>       
    </link>
    <link name="wheel_3">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> 
            <geometry>
                <sphere radius="0.04"/>
            </geometry>
            <material name="wheel_color">
                <color rgba="0.7 0.7 0.7 1.0"/>               
            </material>
        </visual>       
    </link> 
     
    <!-- whell_to_base_link  -->
    <joint name="whell_1_to_base_link" type="continuous">
        <origin xyz="-0.14 0.0 -0.055" rpy="0 0.0 0"/>
        <parent link="base_link"/>
        <child link="wheel_1"/>
        <axis xyz="1 1 0.0"/>
    </joint> 
     <joint name="whell_2_to_base_link" type="continuous">
        <origin xyz="0.07 0.121244 -0.055" rpy="0 0.0 0"/>
        <parent link="base_link"/>
        <child link="wheel_2"/>
        <axis xyz="1 1 0.0"/>
    </joint> 
     <joint name="whell_3_to_base_link" type="continuous">
        <origin xyz=" 0.07 -0.121244 -0.055" rpy="0 0.0 0"/>
        <parent link="base_link"/>
        <child link="wheel_3"/>
        <axis xyz="1 1 0.0"/>
    </joint> 
    
</robot>